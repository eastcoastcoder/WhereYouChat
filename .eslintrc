{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": ["airbnb", "eslint:recommended"],
    "rules": {
        "indent": [
            "error",
            2
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ]
    },
    "globals": {
        "L": false,
        "turf": false,
        "dummyData": false
    }
}